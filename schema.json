                },
                "required": [
                  "name"
                ]
              },
              {
                "not": {
                  "required": [
                    "name"
                  ]
                },
                "required": [
                  "labels"
                ]
              },
              {
                "not": {
                  "anyOf": [
                    {
                      "required": [
                        "name"
                      ]
                    },
                    {
                      "required": [
                        "labels"
                      ]
                    }
                  ]
                }
              }
            ],
            "properties": {
              "labels": {
                "description": "Labels to select the service to forward to",
                "patternProperties": {
                  ".*": {
                    "type": "string"
                  }
                },
                "title": "labels",
                "type": "object"
              },
              "localPort": {
                "description": "Local port to forward from",
                "title": "localPort",
                "type": "integer"
              },
              "name": {
                "description": "Name of the service to forward to",
                "title": "name",
                "type": "string"
              },
              "remotePort": {
                "description": "Remote port to forward to",
                "title": "remotePort",
                "type": "integer"
              }
            },
            "required": [
              "localPort",
              "remotePort"
            ],
            "type": "object"
          }
        ]
      },
      "title": "forward",
      "type": "array"
